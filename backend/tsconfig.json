{
  "compilerOptions": {
    "target": "es6",
    "module": "es6",
    "lib": ["es6"],
    "allowJs": true,
    "noImplicitAny": false,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "outDir": "dist",
    "moduleResolution": "Node"
  },
  "exclude": [
    "node_modules"
  ]
}
